{
  "name": "agent-harbor-gui",
  "version": "0.1.0",
  "description": "Agent Harbor GUI - Cross-platform desktop application for agent workflow management",
  "main": "dist-electron/index.js",
  "author": "Agent Harbor Team",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && npm run build:native-addon && electron-builder",
    "build:dev": "tsc && vite build",
    "build:native-addon": "cd ../crates/ah-gui-core && npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 10",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@agent-harbor/gui-core": "file:../crates/ah-gui-core",
    "electron-store": "^10.0.0",
    "playwright": "^1.49.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "electron": "33.4.11",
    "electron-builder": "^25.1.8",
    "eslint": "9.37.0",
    "prettier": "^3.4.2",
    "typescript": "5.9.3",
    "vite": "^6.0.3",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
