{
  "name": "ah-webui-ssr-sidecar",
  "version": "1.0.0",
  "description": "SSR sidecar server for agent-harbor WebUI providing server-side rendering and REST API proxy",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "yarn vinxi build",
    "build:client": "yarn vinxi build --mode client",
    "build:server": "yarn vinxi build --mode server",
    "dev": "yarn icons:generate && yarn vinxi dev",
    "start": "yarn vinxi start",
    "icons:generate": "node scripts/generate-icon-manifest.ts",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json}",
    "type-check": "tsc --noEmit",
    "knip": "knip",
    "knip:strict": "knip --strict",
    "type-coverage": "type-coverage",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "typeCoverage": {
    "atLeast": 95,
    "detail": true,
    "strict": true,
    "ignoreCatch": false,
    "cache": true,
    "tsconfig": "tsconfig.json",
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/__tests__/**",
      "**/__mocks__/**",
      "e2e-tests/**",
      "tests/**"
    ]
  },
  "keywords": [
    "ssr",
    "solidjs",
    "express",
    "proxy",
    "agent-harbor"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@livestore/livestore": "^0.3.0",
    "@livestore/solid": "^0.3.0",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.0",
    "@solidjs/start": "^1.1.0",
    "solid-js": "^1.9.5",
    "tom-select": "^2.4.3",
    "vinxi": "^0.5.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.37.0",
    "@solidjs/testing-library": "^0.8.10",
    "@storybook/addon-a11y": "^10.1.4",
    "@storybook/addon-docs": "^10.1.4",
    "@storybook/addon-vitest": "^10.1.4",
    "@tailwindcss/vite": "^4.0.7",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "cheerio": "^1.1.2",
    "eslint": "9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^3.7.9",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-storybook": "^10.1.4",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "jsdom": "^27.0.0",
    "knip": "^5.64.1",
    "playwright": "^1.57.0",
    "prettier": "3.6.2",
    "solid-devtools": "^0.34.4",
    "storybook": "^10.1.4",
    "storybook-solidjs-vite": "^10.0.9",
    "tailwindcss": "^4.0.7",
    "type-coverage": "^2.29.7",
    "typescript": "5.9.3",
    "vite": "^7.1.9",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-solid": "^2.11.9",
    "vite-plugin-solid-svg": "^0.8.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22"
  },
  "dependenciesMeta": {
    "@chromatic-com/storybook": {
      "unplugged": true
    },
    "@storybook/addon-a11y": {
      "unplugged": true
    },
    "@storybook/addon-docs": {
      "unplugged": true
    },
    "@storybook/addon-vitest": {
      "unplugged": true
    },
    "storybook": {
      "unplugged": true
    },
    "storybook-solidjs-vite": {
      "unplugged": true
    }
  }
}
