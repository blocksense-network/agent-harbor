{
  "name": "aw-webui-ssr-sidecar",
  "version": "1.0.0",
  "description": "SSR sidecar server for Agents-Workflow WebUI providing server-side rendering and REST API proxy",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "vite build --mode server",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/server.tsx",
    "dev:client": "vite",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json}",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ssr",
    "solidjs",
    "express",
    "proxy",
    "agents-workflow"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.0",
    "@solidjs/start": "^1.1.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^2.0.6",
    "morgan": "^1.10.1",
    "solid-js": "^1.9.5",
    "vinxi": "^0.5.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.8.1",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.3.3",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-solid": "^2.10.2"
  },
  "engines": {
    "node": ">=22"
  }
}
